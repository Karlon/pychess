#: sidepanel/bookPanel.py:136
#, python-format
msgid "Threat analysis by %s"
msgstr "Analiza kërcënimi nga %s"

#: sidepanel/bookPanel.py:137
#, python-format
msgid ""
"%s will identify what threats would exist if it were your opponent's turn to"
" move"
msgstr "%s do të identifikohet se çfarë kërcënime do të ekzistonte nëse do të ishte radha e kundërshtarit tuaj në lëvizje"

